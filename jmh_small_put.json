    "options": {
        "batchsize": 1,
        "warmupiterations": 10,
        "warmuptime": "20ms",
        "iterations": 20,
        "time": "200ms"
    },
    "result.path": "./results",
    "java.library.path": "target/jni-benchmarks-1.0.1-SNAPSHOT-application/jni-benchmarks-1.0.1-SNAPSHOT/lib",
    "jar": "target/jni-benchmarks-1.0.1-SNAPSHOT-benchmarks.nar"
}